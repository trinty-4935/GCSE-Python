nums = []
num1 = int(input("Enter a number: "))
num2 = int(input("Enter a number: "))
num3 = int(input("Enter a number: "))

save = input("Do you want to save the last number you entered? y/n : ")

if save == "n":
    nums.append(num1)
    nums.append(num2)
    print(nums)
else:
    nums.append(num1)
    nums.append(num2)
    nums.append(num3)
    print(nums)
