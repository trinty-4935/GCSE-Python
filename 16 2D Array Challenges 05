EmptyArray = [[0 for column in range(5)] for row in range(2)]
EmptyArray[0][0] = "a"
print(EmptyArray)
column = int(input("How many columns do you want? 1-5 :  "))
row = int(input("How many rows do you want? 1-5 :  "))
EmptyArray = [[0 for column in range(column)] for row in range(row)]
print(EmptyArray)
